"VertexLitGeneric"
{
	"$basetexture"	"Models/Combine_Advisor_Pod/combine_advisor_pod"	
	"$bumpmap"	      "Models/Combine_Advisor_Pod/combine_advisor_pod_normal"

	"$selfillum"	"1"
	"$selfillumtint" 	"[3 .5 .5]"

	"$phong" "1" //this turns on the phong effect
	"$phongexponent"  "20"
	"$phongboost" "4" //use this to increase/decrease the highlight intensity 
	"$phongfresnelranges" "[.1 .5 1]" //this are the highlight intensities at different incidence angles from the camera (first value is perpendicular to the camera and the 3rd value is away from the camera)
}
