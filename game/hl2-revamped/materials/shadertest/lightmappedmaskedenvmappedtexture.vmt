"LightmappedGeneric"
{
	"$basetexture" "shadertest/LightmappedGeneric"
	"$envmap" "shadertest/shadertest_env"
	"$envmapmask" "shadertest/LightmappedGeneric_envmask"
	"$envmapmaskscale" 5

	"$temp" 0
	"$one" 1

	"Proxies"
	{
		// Color green is sinusoidal
		"Sine"
		{
			"resultVar" "$envmaptint[1]"
			"sineperiod" 2.00
			"sinemin" 0
			"sinemax" 1
		}

		"Sine"
		{
			"resultVar" "$alpha"
			"sineperiod" 4.00
			"sinemin" 0
			"sinemax" 1
		}

